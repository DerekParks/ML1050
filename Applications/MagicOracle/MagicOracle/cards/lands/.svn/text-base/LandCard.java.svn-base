package cards.lands;

import cards.Card;

public class LandCard extends Card{

	public LandCard(String name)
	{
		super(name);
	}
	public boolean canProduceWhite()
	{
		if(name.equalsIgnoreCase("Plains"))
		{
			return true;
		}
		return false;
	}
	public boolean canProduceBlack()
	{
		if(name.equalsIgnoreCase("Swamp"))
		{
			return true;
		}
		return false;
	}
	public boolean canProduceGreen()
	{
		if(name.equalsIgnoreCase("Forest"))
		{
			return true;
		}
		return false;
	}
	public boolean canProduceBlue()
	{
		if(name.equalsIgnoreCase("Island"))
		{
			return true;
		}
		return false;
	}
	public boolean canProduceRed()
	{
		if(name.equalsIgnoreCase("Mountain"))
		{
			return true;
		}
		return false;
	}
}
